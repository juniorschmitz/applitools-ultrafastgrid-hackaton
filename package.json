    "test2v1": "cypress run --spec 'cypress/integration/examples/Task2.spec.js'",
    "test2v2": "cypress run --env v2=true --spec 'cypress/integration/examples/Task2.spec.js'",
    "test3v1": "cypress run --spec 'cypress/integration/examples/Task3.spec.js'",
    "test3v2": "cypress run --env v2=true --spec 'cypress/integration/examples/Task3.spec.js'",
    "regressionv1": "cypress run",
    "regressionv2": "cypress run --env v2=true",
    "cypress:open": "cypress open"
  },
